<UserControl x:Class="eWolfPodcasterUI.UserControls.LibraryItem"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:local="clr-namespace:eWolfPodcasterUI.UserControls"
             mc:Ignorable="d" Height="96.896" Width="635.366">
    <Grid Background="{DynamicResource {x:Static SystemColors.ActiveBorderBrushKey}}">
        <Label x:Name="_labelName" Content="{Binding Title}" HorizontalAlignment="Left" VerticalAlignment="Top" Width="416" Height="31" />
        <AccessText x:Name="_labelDescription2" Text="{Binding Description}" TextWrapping="Wrap" HorizontalAlignment="Left" VerticalAlignment="Top" Width="615" Height="66" Margin="10,31,0,0" />
    </Grid>
</UserControl>
